
@{
    //ViewBag.Title = "Detalles";
    Layout = "~/Views/shared/_pivma.main.dashboard.Header.cshtml";
    
}


@using BootstrapSupport
@model Object
<fieldset>
    <legend> <center><b> <small>@ViewBag.Title</small> </b> </center>  </legend>
    <dl> <!-- use this class on the dl if you want horizontal styling http://twitter.github.com/bootstrap/base-css.html#typography  class="dl-horizontal"-->     
        @foreach(var property in Model.VisibleProperties())
        {
            <dt>
                @property.GetLabel().ToSeparatedWords()
            </dt>
            <dd>
                @Html.Display(property.Name)                
            </dd>
        }
    </dl>
</fieldset>
<p>
    @Html.ActionLink("Editar", "Edit_Record", Model.GetIdValue()) |
    @Html.ActionLink("Atras", "Index_Record")
</p>
